<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.sheet_general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>	
                    <type>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.type</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.type_ce</numIndex>
                                        <numIndex index="1">ce</numIndex>
                                    </numIndex>       
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.type_iframe</numIndex>
                                        <numIndex index="1">iframe</numIndex>
                                    </numIndex>    
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.type_media</numIndex>
                                        <numIndex index="1">media</numIndex>
                                    </numIndex>                                                   
                                </items>                                
                                <maxitems>1</maxitems>
                                <size>1</size>                                  
                            </config>
                            <onChange>reload</onChange>
                        </TCEforms>                        
                    </type>                    
                    <ce>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.ce</label>
                            <displayCond>FIELD:type:=:ce</displayCond>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tt_content</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>5</size>
                                <autosizemax>5</autosizemax>
                                <maxitems>100</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </ce>  
                    <link>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.link</label>
                            <displayCond>FIELD:type:!=:ce</displayCond>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <default></default>
                                <wizards type="array">
                                    <_PADDING>2</_PADDING>
                                    <link type="array">
                                        <type>popup</type>
                                        <title>Link</title>
                                        <icon>link_popup.gif</icon>
                                        <script>browse_links.php?mode=wizard&amp;act=file</script>
                                        <params type="array">
                                            <blindLinkOptions>mail,spec,folder,email,file</blindLinkOptions>
                                        </params>
                                        <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                                    </link>
                                </wizards>
                            </config>
                        </TCEforms>
                    </link>                       
                    <linkTitle>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.linkTitle</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </linkTitle>                                     
                </el>
            </ROOT>
        </sDEF>
        <options>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.sheet_options</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>	
                    <width>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.width</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                            </config>
                        </TCEforms>
                    </width>    
                    <autoWidth>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.autoWidth</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </autoWidth>
                    <height>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.height</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                            </config>
                        </TCEforms>
                    </height>  
                    <autoHeight>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.autoHeight</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </autoHeight>  
                    <autoSize>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.autoSize</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </autoSize>  
                    <closeBtn>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.closeBtn</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </closeBtn> 
                    <closeClick>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.closeClick</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </closeClick>   
                    <modal>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.modal</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </modal>   
                    <fitToView>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.fitToView</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </fitToView> 
                    <autoOpen>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.autoOpen</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </autoOpen>                       
                    <padding>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.padding</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>int</eval>
                                <default>20</default>
                            </config>
                        </TCEforms>
                    </padding> 
                    <topRatio>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.topRatio</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <default>0.5</default>
                            </config>
                        </TCEforms>
                    </topRatio>                           
                    <linkClass>
                        <TCEforms>
                            <label>LLL:EXT:medfancyboxcontent/locallang_flexform.xml:medfancyboxcontent.pi_flexform.linkClass</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                            </config>
                        </TCEforms>
                    </linkClass>                                                                                                                                         
                </el>
            </ROOT>
        </options>        
    </sheets>
</T3DataStructure>